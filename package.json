{
  "name": "@kontenbase/kotak-cloud",
  "version": "0.0.1",
  "description": "Client SDK to use Kotak Cloud",
  "main": "dist/main/index.js",
  "types": "dist/main/index.d.ts",
  "module": "dist/module/index.js",
  "scripts": {
    "build": "npm run build:main && npm run build:module && npm run build:umd",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "build:umd": "webpack",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch --verbose false --silent false"
  },
  "author": "Kontenbase",
  "license": "MIT",
  "dependencies": {
    "@kontenbase/sdk": "^0.6.3",
    "axios": "^0.26.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "dotenv": "^16.0.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kontenbase/kotak-cloud.git"
  },
  "keywords": [
    "kotak-cloud",
    "cloud",
    "storage",
    "sdk"
  ],
  "bugs": {
    "url": "https://github.com/kontenbase/kotak-cloud/issues"
  },
  "homepage": "https://github.com/kontenbase/kotak-cloud#readme",
  "jsdelivr": "dist/umd/kotakcloud.js",
  "unpkg": "dist/umd/kotakcloud.js"
}
